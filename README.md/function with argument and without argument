#with argument and with return

def add(a,b):
    c=a+b
    return
add(10,40)

#without argument and with return 
def add():
    a=30
    b=40
    c=a+b
    return c
add()


#with argument and without return

def add(a,b):
    c=a+b
    print(c)
add(10,40)
    

#without argument and without return 
def add():
    a=30
    b=40
    c=a+b
    print(c)
add()
