
math=int(input("enter the marks"))
science=int(input("enter the marks"))
history=int(input("enter the marks"))
marathi=int(input("enter the marks"))
english=int(input("enter the marks"))
def avg():
    avg=(math+science+history+marathi+english)*100/500
    print("your average is",avg)
    if avg>=85:
        print("your grade is A+")
    elif avg>=75:
        print("your grade is A")
    elif avg>=60:
        print("your grade is B")
    elif  avg>=40:
        print("your grade is C")
    else:
        print("you are fail")

avg()
